{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak-Settings": {
    "Audience": "account",
    "ValidIssuer": "http://localhost:8080/realms/case_realm",
    "MetadataAddress": "http://localhost:8080/realms/case_realm/.well-known/openid-configuration",
    "AuthorizationUrl": "http://localhost:8080/realms/case_realm/protocol/openid-connect/auth",
    "RequireHttpsMetadata": false
  },
  "AllowedHosts": "*"
}
